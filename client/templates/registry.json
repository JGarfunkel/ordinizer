        "lib/mapUtils.ts"
      ],
      "dependencies": ["leaflet", "react-leaflet"],
      "requires": ["OrdinizerProvider"]
    },
    "score-display": {
      "name": "score-display",
      "description": "Score visualization with color coding",
      "files": [
